{
  "manifest_version": 2,
  "name": "Workout reminder",
  "version": "1",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["declarativeContent", "storage", "alarms", "notifications"],
  "background": {
      "scripts": ["background.js"],
	  "persistent": false
    },
	"page_action": {
      "default_popup": "index.html",
	  "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "128": "images/icon_128.png"
      }
    },
	"icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "128": "images/icon_128.png"
    },
  "minimum_chrome_version": "28"
}
