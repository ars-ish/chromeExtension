{
  "manifest_version": 2,
  "name": "Workout reminder",
  "version": "1",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["declarativeContent", "storage", "alarms", "notifications"],
  "background": {
      "scripts": ["background.js"],
	  "persistent": false
    },
	"page_action": {
      "default_popup": "index.html"
    },
  "minimum_chrome_version": "28"
}
